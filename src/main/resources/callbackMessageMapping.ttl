@prefix odf: <http://www.w3.org/2002/07/owl#> .

<#ObjectMapping>
    rml:logicalSource [
        rml:source "callbackMessage.xml";
        rml:referenceFormulation ql:XPath;
        rml:iterator "/Objects/Object"
    ];

    rr:subjectMap [
        rr:template "http://ex.com/{id}";
        rr:class odf:Object
    ];

    rr:predicateObjectMap [
        rr:predicate skos:notation;
        rr:objectMap [
            rml:reference "id";
            rr:datatype xsd:String
        ]
    ].
